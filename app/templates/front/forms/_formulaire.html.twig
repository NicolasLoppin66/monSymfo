{% extends "base.html.twig" %}

{% block title %}
    Formulaire
{% endblock %}

{% block body %}

    {{ form_start( form ) }}
    <div class="row">
        <div class="col-md-6">{{ form_row(form.nom) }}</div>
        <div class="col-md-6">{{ form_row(form.email) }}</div>
    </div>
    {{ form_rest( form ) }}
    {{ form_end( form ) }}
    
{% endblock %}